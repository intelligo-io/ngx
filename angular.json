          "options": {
            "tsConfig": [
              "projects/image-viewer/tsconfig.lib.json",
              "projects/image-viewer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
